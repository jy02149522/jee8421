		<result column="createTime" property="createtime" />
		<result column="updateTime" property="updatetime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, loginName, pwd, attribute, tel, email, createTime, updateTime
    </sql>
